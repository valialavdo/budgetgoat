    "@react-native/metro-config": "0.74.87",
    "@react-native/typescript-config": "0.74.87",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.2",
    "@types/jest": "^29.5.8",
