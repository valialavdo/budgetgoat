{
  "name": "BudgetGOAT",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace BudgetGOAT.xcworkspace -scheme BudgetGOAT -configuration Release -destination generic/platform=iOS -archivePath BudgetGOAT.xcarchive archive",
