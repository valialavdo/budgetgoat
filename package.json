    "@react-navigation/bottom-tabs": "^7.4.8",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/native-stack": "^7.3.27",
    "date-fns": "^4.1.0",
    "firebase": "^10.14.1",
    "phosphor-react-native": "^3.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-haptic-feedback": "^2.2.0",
